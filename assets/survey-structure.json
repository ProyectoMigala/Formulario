              "isRequired": true,
              "validators": [
                {
                  "type": "regex",
                  "text": {
                    "es": "Formato de Sección Electoral"
                  },
                  "regex": "^([0-9]{3,4})$"
                }
              ]
            }
          ]
        }
      ],
