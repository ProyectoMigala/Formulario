            {
              "type": "text",
              "name": "clave_elector",
              "title": {
                "es": "Ingresa tu clave de elector"
              },
              "description": {
                "es": "Ese dato lo encuentras en tu credencial para votar del INE, justo en la sección que indica la imagen. Si no cuentas con tu credencial, puedes omitirla.\n\n"
              },
              "validators": [
                {
                  "type": "expression",
                  "text": {
                    "es": "Error en formato"
